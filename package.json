    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-svelte": "^2.39.0",
    "eslint-plugin-unicorn": "53.0.0",
    "prettier-plugin-svelte": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "svelte": "5.0.0-next.135",
    "svelte-check": "^3.7.1",
    "tslib": "^2.6.2",
