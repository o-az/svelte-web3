    "eslint-plugin-unicorn": "51.0.1",
    "prettier-plugin-svelte": "^3.2.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "svelte": "5.0.0-next.135",
    "svelte-check": "^3.6.4",
    "tslib": "^2.6.2",
    "typed-query-selector": "^2.11.0",
    "typescript": "^5.3.3",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.43.0",
