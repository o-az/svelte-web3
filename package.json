    "@wagmi/connectors": "^4.3.10",
    "@wagmi/core": "^2.9.8",
    "tailwindcss": "4.0.0-alpha.15",
    "viem": "^2.10.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240512.0",
